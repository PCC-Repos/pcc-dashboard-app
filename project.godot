[application]

config/name="PCF Dashboard"
run/main_scene="res://splash_screen/splash.tscn"
boot_splash/show_image=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://project.png"
config/macos_native_icon="res://project.icns"
config/windows_native_icon="res://project.ico"
config/quit_on_go_back=false

[autoload]

NotificationServer="*res://systems/notifications/notification_server.gd"
NotificationPopup="*res://ui_classes/NotificationPopup.tscn"

[display]

window/size/width=1366
window/size/height=768
window/size/test_width=1366
window/size/test_height=768
window/dpi/allow_hidpi=true
window/vsync/vsync_via_compositor=true
window/energy_saving/keep_screen_on=false
window/handheld/orientation="sensor"
window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "res://addons/file_protocols/plugin.cfg" )
